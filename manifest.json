{
  "name": "OmniMarks",
  "version": "0.2",
  "manifest_version": 2,
  "description": "__MSG_extension_description__",
  "omnibox": {
    "keyword" : "b"
  },
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },  
  "icons": {
      "16": "img/16.png",
      "48": "img/48.png",
      "128": "img/128.png"
  },
  "default_locale": "en",
  "minimum_chrome_version": "9",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
